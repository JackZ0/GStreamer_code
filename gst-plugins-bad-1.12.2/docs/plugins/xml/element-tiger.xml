<refsect2 id="GstKateTiger" role="struct">
<title>struct GstKateTiger</title>
<indexterm zone="GstKateTiger"><primary>GstKateTiger</primary></indexterm>
<programlisting language="C">struct GstKateTiger {
  GstKateDecoderBase decoder;

  GstPad *katesinkpad;
  GstPad *videosinkpad;
  GstPad *srcpad;

  tiger_renderer *tr;

  gdouble quality;
  gchar *default_font_desc;
  gboolean default_font_effect;
  gdouble default_font_effect_strength;
  guchar default_font_r;
  guchar default_font_g;
  guchar default_font_b;
  guchar default_font_a;
  guchar default_background_r;
  guchar default_background_g;
  guchar default_background_b;
  guchar default_background_a;
  gboolean silent;

  GstVideoFormat video_format;
  gint video_width;
  gint video_height;
  gboolean swap_rgb;
  GstBuffer *render_buffer;
  GstVideoOverlayComposition *composition;

  GMutex *mutex;
  GCond *cond;

  GstSegment video_segment;
  gboolean video_flushing;
  gboolean seen_header;
};
</programlisting>
</refsect2>

</refsect1>
