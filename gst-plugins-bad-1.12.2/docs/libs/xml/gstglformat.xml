<programlisting language="C"><link linkend="guint"><returnvalue>guint</returnvalue></link>
gst_gl_format_type_n_bytes (<parameter><link linkend="guint"><type>guint</type></link> format</parameter>,
                            <parameter><link linkend="guint"><type>guint</type></link> type</parameter>);</programlisting>
